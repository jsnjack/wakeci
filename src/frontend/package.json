    "cypress": "^13.6.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-vue": "^9.18.1",
    "javascript-time-ago": "^2.5.9",
    "lodash": "^4.17.21",
    "material-dynamic-colors": "^1.1.0",
    "minimist": "^1.2.8",
    "mitt": "^3.0.1",
    "prettier": "^3.1.0",
