    "cypress": "^13.3.2",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "javascript-time-ago": "^2.5.9",
    "lodash": "^4.17.21",
    "material-dynamic-colors": "^1.0.5",
    "minimist": "^1.2.8",
    "mitt": "^3.0.1",
    "prettier": "^3.0.3",
    "saas": "^1.0.0",
