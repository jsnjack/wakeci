        "cypress": "^14.5.2",
        "eslint": "^9.31.0",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-vue": "^9.32.0",
        "javascript-time-ago": "^2.5.11",
        "lodash": "^4.17.21",
        "material-dynamic-colors": "^1.1.2",
        "minimist": "^1.2.8",
        "mitt": "^3.0.1",
        "prettier": "^3.6.2",
        "vite": "^7.0.5",
        "vue": "^3.5.17",
        "vue-router": "^4.5.1",
        "vuex": "^4.1.0"
