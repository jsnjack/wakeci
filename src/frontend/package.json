{
    "name": "frontend",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "scripts": {
        "serve": "vite",
        "build": "vite build",
        "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
        "format": "prettier .  --write",
        "test": "cypress run --browser=chrome --headless --config-file=cypress_prod.js",
